{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html",
      "has": [
        {
          "type": "host",
          "value": "crmpennyanddebt.in"
        }
      ]
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
