{
  "version": 2,
  "name": "pennyanddebt",
  "buildCommand": "cd crm-frontend && npm run build",
  "outputDirectory": "crm-frontend/dist",
  "installCommand": "cd crm-frontend && npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.pennyanddebt.in/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}